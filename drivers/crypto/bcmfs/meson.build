if not is_linux
    build = false
    reason = 'only supported on Linux'
    subdir_done()
endif

deps += ['eal', 'bus_vdev']
sources = files(
        'bcmfs_logs.c',
        'bcmfs_device.c',
        'bcmfs_vfio.c',
        'bcmfs_qp.c',
        'hw/bcmfs4_rm.c',
        'hw/bcmfs5_rm.c',
        'hw/bcmfs_rm_common.c',
        'bcmfs_sym_pmd.c',
        'bcmfs_sym_capabilities.c',
        'bcmfs_sym_session.c',
        'bcmfs_sym.c',
        'bcmfs_sym_engine.c',
)
